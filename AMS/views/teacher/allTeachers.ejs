<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8 ">
      <h3 class="text-center mb-3" >Teachers</h3>
      <table class="table text-center py-4">
        <thead>
          <tr>
            <th>Teacher Name</th>
          </tr>
        </thead>
        <tbody>
          <% teachers.forEach(teacher => { %>
          <tr>
            <td><%= teacher.name %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</div>